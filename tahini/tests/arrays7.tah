fun isEven(n) {
  while (n >= 2) {
    n = n - 2;
  }
  return n == 0;
}

fun filterEven(arr) {
  var oddArr = [];
  for (var i = 0; i < len(arr); i = i + 1) {
    if (isEven(arr[i])) {
      oddArr = oddArr + [arr[i]];
    }
  }
  return oddArr;
}

var arr = [1, 2, 3, 4, 5];
print filterEven(arr);
